<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.yeolmok.samplepluginswift" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>CordovaSamplePluginSwift</name>
    <js-module name="CordovaSamplePluginSwift" src="www/CordovaSamplePluginSwift.js">
        <clobbers target="sample" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="CordovaSamplePluginSwift">
                <param name="ios-package" value="CordovaSamplePluginSwift" />
            </feature>
        </config-file>
        <source-file src="src/ios/CordovaSamplePluginSwift.swift" />
    </platform>
</plugin>
